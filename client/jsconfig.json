{
  "compilerOptions": {
    "moduleResolution": "bundler",
    "target": "ESNext",
    "module": "ESNext",
    "resolveJsonModule": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "checkJs": false,
    "types": ["svelte"]
  },
  "include": ["src/**/*.js", "src/**/*.svelte"]
}
